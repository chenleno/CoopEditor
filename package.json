    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^5.2.6",
    "html-webpack-plugin": "^5.0.0",
    "postcss-loader": "^5.3.0",
    "serve": "^11.3.2",
